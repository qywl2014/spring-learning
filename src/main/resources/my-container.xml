<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
>

    <bean id="1" class="com.wulang.xml.domain.GenericFunction">
        <constructor-arg name="a" value="1"/>
    </bean>
    <bean id="2" class="com.wulang.xml.domain.GenericFunction">
        <constructor-arg name="a" value="1"/>
        <constructor-arg name="b" value="2"/>
        <constructor-arg name="orange" ref="orange"/>
        <property name="apple" ref="apple"/>
        <property name="c" value="3"/>
    </bean>
    <bean id="myFactoryBean" class="com.wulang.xml.domain.GenericFunctionFactoryBean"/>
    <bean id="apple" class="com.wulang.xml.domain.Apple"/>
    <bean id="orange" class="com.wulang.xml.domain.Orange"/>
</beans>